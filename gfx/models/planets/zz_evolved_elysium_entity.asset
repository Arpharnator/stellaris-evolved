#main seam entity
entity = {
	name = "pc_tec_elysium_seam"
	cull_radius = 500
		
	locator = { name = "loc101" position = { 0 0 0 }rotation = { 90 -15 45 } }
	
	#tell the game what entities to attach to each locator point
	attach = { "loc101" = "pc_tec_elysium_seam_ring_01_entity"}
	
	default_state = "idle"
	
	scale = 1
}

entity = {
	name = "pc_tec_elysium_seam_ring_01_entity"
	cull_radius = 500
		
	#create the positions for a planet plane ring
	#the format of locator is:
	#name is used to name the locator point
	#position is where it is in relation to the center of the entity
	#positions are formatted as x y z positions, where x and z are on the horizontal plane and y is for height
	#rotation is the formatted the same way, with rotation along the x y z axes
	locator = { name = "loc101" position = { 250 0 0 }rotation = { 90 0 0 } }
	locator = { name = "loc102" position = { 216.5 0 125 }rotation = { 60 0 0 } }
	locator = { name = "loc103" position = { 125 0 216.5 }rotation = { 30 0 0 } }
	locator = { name = "loc104" position = { 0 0 250 }rotation = { 0 0 0 } }
	locator = { name = "loc105" position = { -125 0 216.5 }rotation = { -30 0 0 } }
	locator = { name = "loc106" position = { -216.5 0 125 }rotation = { -60 0 0 } }
	locator = { name = "loc107" position = { -250 0 0 }rotation = { -90 0 0 } }
	locator = { name = "loc108" position = { -216.5 0 -125 }rotation = { -120 0 0 } }
	locator = { name = "loc109" position = { -125 0 -216.5 }rotation = { -150 0 0 } }
	locator = { name = "loc110" position = { 0 0 -250 }rotation = { -180 0 0 } }
	locator = { name = "loc111" position = { 125 0 -216.5 }rotation = { -210 0 0 } }
	locator = { name = "loc112" position = { 216.5 0 -125 }rotation = { -240 0 0 } }
	
	#tell the game what entities to attach to each locator point
	attach = { "loc101" = "pc_tec_elysium_seam_segment_01_entity"}
	attach = { "loc102" = "pc_tec_elysium_seam_segment_01_entity"}
	attach = { "loc103" = "pc_tec_elysium_seam_segment_01_entity"}
	attach = { "loc104" = "pc_tec_elysium_seam_segment_01_entity"}
	attach = { "loc105" = "pc_tec_elysium_seam_segment_01_entity"}
	attach = { "loc106" = "pc_tec_elysium_seam_segment_01_entity"}
	attach = { "loc107" = "pc_tec_elysium_seam_segment_01_entity"}
	attach = { "loc108" = "pc_tec_elysium_seam_segment_01_entity"}
	attach = { "loc109" = "pc_tec_elysium_seam_segment_01_entity"}
	attach = { "loc110" = "pc_tec_elysium_seam_segment_01_entity"}
	attach = { "loc111" = "pc_tec_elysium_seam_segment_01_entity"}
	attach = { "loc112" = "pc_tec_elysium_seam_segment_01_entity"}
	
	state = { name = "idle" state_time = 100 time_offset = { 0 100 }
		start_event = { trigger_once = yes sound = { soundeffect = "ringworld_hum_idle_01" } }
		start_event = { trigger_once = yes sound = { soundeffect = "ringworld_hum_idle_02" } }
	}
	
	default_state = "idle"
	
	scale = 0.4875 #0.75 #0.25
}

